{
    "name": "kelunik/acme",
    "description": "ACME library written in PHP.",
    "license": "MIT",
    "authors": [
        {
            "name": "Niklas Keller",
            "email": "me@kelunik.com"
        }
    ],
    "keywords": [
        "ACME",
        "letsencrypt",
        "certificate",
        "https",
        "encryption",
        "ssl",
        "tls"
    ],
    "autoload": {
        "psr-4": {
            "Kelunik\\Acme\\": "lib"
        },
        "files": [
            "lib/functions.php"
        ]
    },
    "require": {
        "amphp/artax": "^2",
        "namshi/jose": "^7",
        "sabre/uri": "^1"
    },
    "require-dev": {
        "phpunit/phpunit": "^4|^5",
        "phpdocumentor/phpdocumentor": "^2"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.4-dev"
        }
    }
}
